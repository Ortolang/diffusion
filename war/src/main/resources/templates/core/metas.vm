<div class="panel panel-default">
  <div class="panel-heading">
  	<h3 class="panel-title"><strong>Metadatas of object</strong></h3>
  </div>
  <div class="panel-body">
	  <table class="table table-condensed table-hover">
		<thead>
       		<tr>
    	   		<th>#</th>
	       		<th>Key</th>
       			<th>Links</th>
      		</tr>
   		</thead>
   		<tbody>
		#foreach( $entry in $entity.entries.keySet() )
			#set( $count = $foreach.count + $entity.start - 1 )
			<tr>
				<td>$count</td>
				<td>$entry</td>
				<td>
					<div class="btn-group">
   					#foreach( $link in $entity.entries.get($entry) )
   						<a class="btn btn-default btn-xs" href="$link.uri">$link.rel</a> 
   					#end
   					</div>
   				</td>
   			</tr>
		#end
	  </tbody>
    </table>
  </div>
</div>