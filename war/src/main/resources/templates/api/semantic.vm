<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title"><strong>Search Objects</strong></h3>
  </div>
  <div class="panel-body">
    <form class="form-horizontal" role="form" method="get" action="">
  		<div class="form-group">
  			<div class="col-md-6">
  				#set ($query = $params.getFirst("query"))
	    		<input type="text" class="form-control" id="query" name="query" placeholder="Query" value="#if($query)$query#end ">
	    	</div>
	    	<button type="submit" class="btn btn-default">Search</button>
  		</div>
  	</form>
  	<pre>$entity</pre>
  </div>
</div> 
