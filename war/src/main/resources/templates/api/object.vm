<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title"><strong>Object Detail</strong></h3>
  </div>
  <div class="panel-body">
    <div class="row">
	  <div class="col-md-2"><p class="text-right"><strong>Key</strong></p></div>
	  <div class="col-md-10">$entity.key</div>
    </div>
    <div class="row">
	  <div class="col-md-2"><p class="text-right"><strong>Service</strong></p></div>
	  <div class="col-md-10">$entity.service</div>
    </div>
    <div class="row">
	  <div class="col-md-2"><p class="text-right"><strong>Type</strong></p></div>
	  <div class="col-md-10">$entity.type</div>
    </div>
    <div class="row">
	  <div class="col-md-2"><p class="text-right"><strong>Internal ID</strong></p></div>
	  <div class="col-md-10">$entity.id</div>
    </div>
  </div>
</div> 
<ul class="pagination">
	#foreach( $link in $entity.links )
		<li><a href="$link.uri">$link.rel</a></li> 
	#end
</ul>