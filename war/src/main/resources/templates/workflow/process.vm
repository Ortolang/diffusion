<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title"><strong>Process Detail</strong></h3>
  </div>
  <div class="panel-body">
    <div class="row">
	  <div class="col-md-2"><p class="text-right"><strong>Key</strong></p></div>
	  <div class="col-md-10">$entity.key</div>
    </div>
    <div class="row">
	  <div class="col-md-2"><p class="text-right"><strong>Type</strong></p></div>
	  <div class="col-md-10">$entity.type</div>
    </div>
    <div class="row">
	  <div class="col-md-2"><p class="text-right"><strong>Initier</strong></p></div>
	  <div class="col-md-10">$entity.initier</div>
    </div>
    <div class="row">
	  <div class="col-md-2"><p class="text-right"><strong>Name</strong></p></div>
	  <div class="col-md-10">$entity.name</div>
    </div>
    <div class="row">
	  <div class="col-md-2"><p class="text-right"><strong>Start</strong></p></div>
	  <div class="col-md-10">$entity.start</div>
    </div>
    <div class="row">
	  <div class="col-md-2"><p class="text-right"><strong>Stop</strong></p></div>
	  <div class="col-md-10">$entity.stop</div>
    </div>
    <div class="row">
	  <div class="col-md-2"><p class="text-right"><strong>Status</strong></p></div>
	  <div class="col-md-10">$entity.status</div>
    </div>
    <div class="row">
	  <div class="col-md-2"><p class="text-right"><strong>Log</strong></p></div>
	  <div class="col-md-10">$entity.log</div>
    </div>
  </div>
</div> 
<ul class="pagination">
	#foreach( $link in $entity.links )
		<li><a href="$link.uri">$link.rel</a></li> 
	#end
</ul>