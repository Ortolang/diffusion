  <process id="test" name="My Test Hello World process" isExecutable="true">
    <startEvent id="startevent1" name="Start" activiti:formKey="test-process-start-form.json"></startEvent>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="servicetask1"></sequenceFlow>
    <serviceTask id="servicetask1" name="Say Hello 1" activiti:class="fr.ortolang.diffusion.runtime.engine.task.HelloWorldTask"></serviceTask>
    <sequenceFlow id="flow2" sourceRef="servicetask1" targetRef="servicetask2"></sequenceFlow>
    <serviceTask id="servicetask2" name="Say Hello 2" activiti:async="true" activiti:class="fr.ortolang.diffusion.runtime.engine.task.HelloWorldTask">
      <extensionElements>
        <activiti:field name="delay">
          <activiti:string><![CDATA[60000]]></activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow3" sourceRef="servicetask2" targetRef="servicetask3"></sequenceFlow>
    <serviceTask id="servicetask3" name="Say Hello 3" activiti:async="true" activiti:class="fr.ortolang.diffusion.runtime.engine.task.HelloWorldTask">
      <extensionElements>
        <activiti:field name="delay">
          <activiti:string><![CDATA[60000]]></activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow4" sourceRef="servicetask3" targetRef="usertask1"></sequenceFlow>
    <userTask id="usertask1" name="Confirm Say Hello" activiti:candidateGroups="moderators" activiti:formKey="test-process-confirm-form.json">
      <documentation>Do me need to say hello again ??</documentation>
    </userTask>
    <sequenceFlow id="flow5" sourceRef="usertask1" targetRef="exclusivegateway1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow6" sourceRef="exclusivegateway1" targetRef="servicetask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${helloAgain}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow7" sourceRef="exclusivegateway1" targetRef="terminateendevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!helloAgain}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="servicetask4" name="Say Hello 4" activiti:async="true" activiti:class="fr.ortolang.diffusion.runtime.engine.task.HelloWorldTask">
      <extensionElements>
        <activiti:field name="delay">
          <activiti:string><![CDATA[60000]]></activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow8" sourceRef="servicetask4" targetRef="servicetask5"></sequenceFlow>
    <serviceTask id="servicetask5" name="Say Hello 5" activiti:async="true" activiti:class="fr.ortolang.diffusion.runtime.engine.task.HelloWorldTask">
      <extensionElements>
        <activiti:field name="delay">
          <activiti:string><![CDATA[60000]]></activiti:string>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow9" sourceRef="servicetask5" targetRef="terminateendevent1"></sequenceFlow>
    <endEvent id="terminateendevent1" name="TerminateEndEvent">
      <terminateEventDefinition></terminateEventDefinition>
    </endEvent>
  </process>
