{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": false,
    "description": "Schema for CMDI envelop",
    "id": "http://www.clarin.eu/cmd/1",
    "properties": {
        "CMD": {
            "additionalProperties": false,
            "id": "http://www.clarin.eu/cmd/1/CMD",
            "properties": {
                "CMDVersion": {
                    "enum": [
                        "1.2"
                    ],
