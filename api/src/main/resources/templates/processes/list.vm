<div class="panel panel-default">
	<div class="panel-heading">
		<h3 class="panel-title"><strong>List of processes for initier: $params.get('initier')</strong></h3>
	</div>
	<div class="panel-body">
		<table class="table table-condensed table-hover">
			<thead>
				<tr>
					<th>#</th>
					<th>Key</th>
					<th>Name</th>
					<th>Type</th>
					<th>State</th>
					<th>Actions</th>
				</tr>
			</thead>
			<tbody>
			#foreach( $entry in $entity.entries )
				<tr>
					<td>$foreach.count</td>
					<td>$entry.key</td>
					<td>$entry.name</td>
					<td>$entry.type</td>
					<td>$entry.state</td>
					<td>
						<div class="btn-group"><a class="btn btn-default btn-xs" href="processes/$entry.key">show</a></div>
					</td>
				</tr>
			#end
			</tbody>
		</table>
	</div>
</div>
<div class="panel panel-default">
	<div class="panel-heading">
		<h3 class="panel-title"><strong>Create a new simple-publish process</strong></h3>
	</div>
	<div class="panel-body">
		<form class="form-horizontal" role="form" action="" method="post">
			<div class="form-group">
				<label for="type" class="col-sm-2 control-label">Type</label>
				<div class="col-sm-2">
					<input type="text" class="form-control input-sm" id="type" name="type" value="simple-publication" readonly/>
				</div>
			</div>
			<div class="form-group">
				<label for="name" class="col-sm-2 control-label">Name</label>
				<div class="col-sm-4">
					<input type="text" class="form-control input-sm" id="name" name="name" placeholder="Enter Process Name"/>
				</div>
			</div>
			<div class="form-group">
				<label for="keys" class="col-sm-2 control-label">Keys</label>
				<div class="col-sm-8">
					<input type="text" class="form-control input-sm" id="key" name="keys" placeholder="Enter Keys to publish"/>
				</div>
			</div>
			<div class="form-group">
				<div class="col-sm-offset-2 col-sm-10">
					<button type="submit" class="btn btn-default btn-sm">Create Process</button>
				</div>
			</div>
		</form>
	</div>
</div>
<div class="panel panel-default">
	<div class="panel-heading">
		<h3 class="panel-title"><strong>Create a new import-workspace process</strong></h3>
	</div>
	<div class="panel-body">
		<form class="form-horizontal" role="form" action="" method="post" enctype="multipart/form-data">
			<div class="form-group">
				<label for="type" class="col-sm-2 control-label">Process Type</label>
				<div class="col-sm-2">
					<input type="text" class="form-control input-sm" id="type" name="type" value="import-workspace" readonly/>
				</div>
			</div>
			<div class="form-group">
				<label for="name" class="col-sm-2 control-label">Process Name</label>
				<div class="col-sm-4">
					<input type="text" class="form-control input-sm" id="name" name="name" placeholder="Enter Process Name"/>
				</div>
			</div>
			<div class="form-group">
				<label for="wsid" class="col-sm-2 control-label">Workspace ID</label>
				<div class="col-sm-4">
					<input type="text" class="form-control input-sm" id="wsid" name="workspace-key" placeholder="Enter Workspace ID"/>
				</div>
			</div>
			<div class="form-group">
				<label for="wstype" class="col-sm-2 control-label">Workspace Type</label>
				<div class="col-sm-4">
					<input type="text" class="form-control input-sm" id="wstype" name="workspace-type" placeholder="Enter Workspace Type"/>
				</div>
			</div>
			<div class="form-group">
				<label for="wsname" class="col-sm-2 control-label">Workspace Name</label>
				<div class="col-sm-8">
					<input type="text" class="form-control input-sm" id="wsname" name="workspace-name" placeholder="Enter Workspace Name"/>
				</div>
			</div>
			<div class="form-group">
				<label for="bag-hash" class="col-sm-2 control-label">Workspace Bag</label>
				<div class="col-sm-8">
					<input type="file" id="bag-hash" name="bag-hash"/>
				</div>
			</div>
			<div class="form-group">
				<div class="col-sm-offset-2 col-sm-10">
					<button type="submit" class="btn btn-default btn-sm">Create Process</button>
				</div>
			</div>
		</form>
	</div>
</div>