<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title"><strong>Specific informations about this process</strong></h3>
  </div>
  <div class="panel-body">
    <div class="row">
	  <div class="col-md-2"><p class="text-right"><strong>Type</strong></p></div>
	  <div class="col-md-10">$entity.object.type</div>
    </div>
    <div class="row">
	  <div class="col-md-2"><p class="text-right"><strong>Initier</strong></p></div>
	  <div class="col-md-10">$entity.object.initier</div>
    </div>
    <div class="row">
	  <div class="col-md-2"><p class="text-right"><strong>Name</strong></p></div>
	  <div class="col-md-10">$entity.object.name</div>
    </div>
    <div class="row">
	  <div class="col-md-2"><p class="text-right"><strong>State</strong></p></div>
	  <div class="col-md-10">$entity.object.state</div>
    </div>
    #foreach( $param in $entity.object.params.keySet() )
    <div class="row">
      <div class="col-md-2"><p class="text-right"><strong>$param</strong></p></div>
      <div class="col-md-10">$entity.object.params.get($param)</div>
    </div>
    #end
    <div class="row">
	  <div class="col-md-2"><p class="text-right"><strong>Log</strong></p></div>
	  <div class="col-md-10"><pre>$entity.object.log</pre></div>
    </div>
  </div>
</div> 